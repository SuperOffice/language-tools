{
  "name": "vscode-superoffice",
  "displayName": "vscode-superoffice",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-superoffice.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscode-superoffice.signIn",
        "title": "Sign In",
        "icon": "$(log-in)"
      },
      {
        "command": "vscode-superoffice.signOut",
        "title": "Sign Out",
        "icon": "$(log-out)"
      },
      {
        "command": "vscode-superoffice.showScriptInfo",
        "title": "ShowScriptInfo"
      },
      {
        "command": "vscode-superoffice.previewScript",
        "title": "Preview"
    }
    ],
    "viewsWelcome": [
      {
        "view": "SuperOfficeView",
        "contents": "You are not logged in to SuperOffice [learn more](https://www.npmjs.com/).\n[Login](command:vscode-superoffice.signIn)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "SuperOfficeViewsContainer",
          "title": "SuperOffice",
          "icon": "/resources/logo.svg"
        }
      ]
    },
    "views": {
      "SuperOfficeViewsContainer": [
        {
          "id": "SuperOfficeView",
          "name": "Option1"
        },
        {
          "id": "OnlineTreeView",
          "name": "Option2"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "vscode-superoffice.signIn",
          "when": "view == OnlineTreeView && !isLoggedIn",
          "group": "navigation",
          "icon": "$(log-in)"
        },
        {
          "command": "vscode-superoffice.signOut",
          "when": "view == OnlineTreeView && isLoggedIn",
          "group": "navigation",
          "icon": "$(log-out)"
        }
      ],
      "view/item/context": [
        {
            "command": "vscode-superoffice.previewScript",
            "group": "0_script",
            "when": "view == OnlineTreeView && viewItem == script"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.6",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "node-fetch": "^2.7.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "openid-client": "^5.5.0"
  }
}
